<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Main</servlet-name>
  	<display-name>Main</display-name>
  	<description></description>
  	<servlet-class>br.com.giovanildo.main.Main</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Teste</servlet-name>
  	<display-name>Teste</display-name>
  	<description></description>
  	<servlet-class>br.com.giovanildo.teste.Teste</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Inicial</servlet-name>
  	<display-name>Inicial</display-name>
  	<description></description>
  	<servlet-class>br.com.giovanildo.servlet.Inicial</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Main</servlet-name>
  	<url-pattern>/lombras</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Teste</servlet-name>
  	<url-pattern>/teste</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Inicial</servlet-name>
  	<url-pattern>/inicial</url-pattern>
  </servlet-mapping>
</web-app>
